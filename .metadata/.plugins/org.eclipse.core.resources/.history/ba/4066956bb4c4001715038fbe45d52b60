    }//GEN-LAST:event_jTextField12ActionPerformed

    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton7ActionPerformed
        
        setLabelText();
        Sql_Connect sq = new Sql_Connect();
        int rowIndex = jTable3.getSelectedRow();
        //isbn =(String) jTable3.getValueAt(rowIndex, 0);
        
        //name = (String) jTable3.getValueAt(rowIndex, 2);
        if(rowIndex == -1)
            message = "Please Select the book row which you want to check-in";
        else{
        loan_id = (String) jTable3.getValueAt(rowIndex, 3);
        card_no = (String) jTable3.getValueAt(rowIndex, 1);
        fine_amt = (Double) jTable3.getValueAt(rowIndex, 5);
            if(fine_amt>0){
                jDialog1.setVisible(true);
                jLabel27.setText("Fine Amount: "+fine_amt);
            }
            else{
                message = sq.checkIn(loan_id);
                model1.removeRow(rowIndex);
            }
        }
        
        jLabel12.setText(message);
        sq.disConnect();
    }//GEN-LAST:event_jButton7ActionPerformed

    private void jButton8ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton8ActionPerformed
        setLabelText();
        Sql_Connect sq = new Sql_Connect();
        jDialog1.dispose();
        message = "Thank You..!! Your book has been checked in.";
        jLabel12.setText(message);
        int rowIndex = jTable3.getSelectedRow();
        loan_id = (String) jTable3.getValueAt(rowIndex, 3);
        fine_amt = (Double) jTable3.getValueAt(rowIndex, 5);
        sq.checkIn(loan_id);
        sq.fineInsert(loan_id,fine_amt,false);
        model1.removeRow(rowIndex);
        sq.disConnect();
    }//GEN-LAST:event_jButton8ActionPerformed

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
        setLabelText();
        Sql_Connect sq = new Sql_Connect();
        jTabbedPane2.setSelectedIndex(3);
        jDialog1.dispose();
        int rowIndex = jTable3.getSelectedRow();
        loan_id = (String) jTable3.getValueAt(rowIndex, 3);
        card_no = (String) jTable3.getValueAt(rowIndex, 1);
        fine_amt = (Double) jTable3.getValueAt(rowIndex, 5);
        jTextField14.setText(card_no);
        sq.checkIn(loan_id);
        sq.fineInsert(loan_id,fine_amt,false);
        sq.finesDisplay(card_no,false,model3);
        sq.disConnect();
    }//GEN-LAST:event_jButton4ActionPerformed

    private void jButton10ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton10ActionPerformed
        setLabelText();
        Sql_Connect sq = new Sql_Connect();
        int rowIndex = jTable1.getSelectedRow();
        if(rowIndex == -1){
            message = "Please Select the book row which you want to check-in";
            jLabel24.setText(message);
        }
        else{
        jTabbedPane1.setSelectedIndex(1);
        jTabbedPane2.setSelectedIndex(2);
        isbn = (String) jTable1.getValueAt(rowIndex, 0);
        resetAll();
        jTextField5.setText(isbn);
        message = sq.checkInDisplay(isbn,card_no,fname,lname,model1);
        jLabel12.setText(message);
        }
        sq.disConnect();
    }//GEN-LAST:event_jButton10ActionPerformed

    private void jButton11ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton11ActionPerformed
        setLabelText();
        Sql_Connect sq = new Sql_Connect();
        int rowIndex = jTable1.getSelectedRow();
        if(rowIndex == -1){
            message = "Please Select the book row which you want to check-out";
            jLabel24.setText(message);
        }
        else{
            jTabbedPane1.setSelectedIndex(1);
            jTabbedPane2.setSelectedIndex(1);
            isbn = (String) jTable1.getValueAt(rowIndex, 0);
            jTextField1.setText(isbn);
            branch_id = (String) jTable1.getValueAt(rowIndex, 3);
            jTextField3.setText(branch_id);
            resetAll();
        }
        sq.disConnect();
    }//GEN-LAST:event_jButton11ActionPerformed

    private void jButton12ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton12ActionPerformed
            resetAll();
            Sql_Connect sq = new Sql_Connect();
            sq.fineUpdate();
            jLabel31.setText("Fines Updated");
            sq.disConnect();
    }//GEN-LAST:event_jButton12ActionPerformed

    private void jButton9ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton9ActionPerformed
        setLabelText();
        resetAll();
        Sql_Connect sq = new Sql_Connect();
        card_no= jTextField14.getText();
        boolean show_previous = jCheckBox4.isSelected();
        if(card_no.isEmpty())
            message="Enter Card Number";
        else{
            message= sq.finesDisplay(card_no,show_previous,model3);
        }

        jLabel31.setText(message);
        sq.disConnect();
    }//GEN-LAST:event_jButton9ActionPerformed

