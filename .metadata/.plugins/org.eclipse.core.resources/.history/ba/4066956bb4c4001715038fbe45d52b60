    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed
            int rowIndex = jTable2.getSelectedRow();
            Sql_Connect sq = new Sql_Connect();
            if(rowIndex==-1)
                message = "Select a row to pay fine";
            else{
                loan_id = (String) jTable2.getValueAt(rowIndex, 0);
                paid = (String) jTable2.getValueAt(rowIndex, 3);
                book_returned = (String) jTable2.getValueAt(rowIndex, 4);
                message =sq.paidUpdate(loan_id,paid,book_returned);    
            }
            if(message.equals("Paid is updated"))
                       model3.removeRow(rowIndex);
        jLabel31.setText(message);
        sq.disConnect();
    }//GEN-LAST:event_jButton5ActionPerformed

    private void jButton13ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton13ActionPerformed
        // TODO add your handling code here:
        card_no = jTextField14.getText();
        String amt;
        Sql_Connect sq = new Sql_Connect();
        if(card_no.isEmpty())
            message="Enter a Card Number";
        else{
            amt = sq.fineTotal(card_no);
            message = "Total Amount of Fine to be paid is: "+ amt;
        }
            jLabel31.setText(message);
            sq.disConnect();
    }//GEN-LAST:event_jButton13ActionPerformed
    
    private void resetAll(){
        
        for (int i = model1.getRowCount() - 1; i > -1; i--) {
                    model1.removeRow(i);
        }
        for (int i = model2.getRowCount() - 1; i > -1; i--) {
                    model2.removeRow(i);
        }
        for (int i = model3.getRowCount() - 1; i > -1; i--) {
                    model3.removeRow(i);
        }
    }
    
    private void setLabelText(){
        jLabel24.setText("");
        jLabel23.setText("");
        jLabel12.setText("");
        jLabel22.setText("");
        jLabel31.setText("");
    }
    
    private void setTextField(){
        jTextField1.setText("");
        jTextField3.setText("");
        jTextField4.setText("");
        jTextField8.setText("");
        jTextField15.setText("");
        jTextField11.setText("");
        jTextField16.setText("");
        jTextField9.setText("");
        jTextField17.setText("");
        jTextField10.setText("");
        jTextField18.setText("");
    }
    
