    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
        setLabelText();
        resetAll();
        Sql_Connect sq = new Sql_Connect();
        isbn = jTextField5.getText();
        fname = jTextField6.getText();
        lname = jTextField12.getText();
        card_no = jTextField7.getText();
        if(isbn.isEmpty() && fname.isEmpty() && card_no.isEmpty() && lname.isEmpty())
            message = "Enter atleast one Field";
        else{
            message = sq.checkInDisplay(isbn,card_no,fname,lname,model1);
            setTextField();
        }
        
        jLabel12.setText(message);
        sq.disConnect();
    }//GEN-LAST:event_jButton2ActionPerformed

