    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
        setLabelText();
        resetAll();
        Sql_Connect sq = new Sql_Connect();
        fname = jTextField8.getText();
        lname = jTextField15.getText();
        ssn = jTextField11.getText();
        address = jTextField16.getText();
        city =jTextField9.getText();
        state = jTextField17.getText();
        email= jTextField10.getText();
        phone = jTextField18.getText();
        if(fname.isEmpty() || lname.isEmpty()|| ssn.isEmpty() || address.isEmpty() || city.isEmpty() || state.isEmpty()){
            message = "Fields marked * are mandatory !!";
        }
        else{
            message = sq.borrowerNew(fname,lname,ssn,address,city,state,email,phone);
            if(message.equals( "Please provide the SSN in the number format XXX-XX-XXX"))
                flag=0;
            else
                flag =1;
        }
        
        jLabel22.setText(message);
        if(flag ==1){
            setTextField();
        }
        sq.disConnect();
    }//GEN-LAST:event_jButton3ActionPerformed

