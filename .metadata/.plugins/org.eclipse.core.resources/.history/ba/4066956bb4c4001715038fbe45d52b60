    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton6ActionPerformed
        setLabelText();
        resetAll();
        Sql_Connect sq = new Sql_Connect();
        isbn = jTextField1.getText();
        if(jTextField3.getText().isEmpty())
        {
        	branch_id="1";
        }
        else
        { branch_id = jTextField3.getText();
        }
        	
        card_no = jTextField4.getText();
        if(isbn.isEmpty() || card_no.isEmpty())
            message = "Fields marked * are mandatory";
        else{
            message = sq.checkOut(isbn,branch_id,card_no);
            }
        jLabel23.setText(message);
        sq.disConnect();
    }//GEN-LAST:event_jButton6ActionPerformed

