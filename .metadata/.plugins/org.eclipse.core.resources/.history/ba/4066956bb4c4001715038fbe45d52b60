    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
    	 resetAll();
	        setLabelText();
	        Sql_Connect sq = new Sql_Connect();
	        isbn=jTextField13.getText();
	        
	        //System.out.println(isbn +" "+ author+" "+title+" " );
	        
	        if(isbn.isEmpty()){
	            message="Enter something to search";
	        }
	        else{
	            message = sq.searchDisplay(isbn,model2);
	        }
	        jLabel24.setText(message);
	        sq.disConnect();
    }//GEN-LAST:event_jButton1ActionPerformed

